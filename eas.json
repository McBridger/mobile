{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "e2e": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "e2e",
        "WITH_MOCK": "true",
        "ENCRYPTION_SALT": "0123456789abcdef",
        "MNEMONIC_LENGTH": "2"
      }
    },
    "prod": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "prod"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
