{
  "name": "rn-bridger",
  "main": "expo-router/entry",
  "version": "0.1.5",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:prebuild": "APP_VARIANT=dev expo prebuild --clean --platform android",
    "android:check": "APP_VARIANT=dev bunx expo doctor",
    "android:dev": "export APP_VARIANT=dev && expo prebuild --clean --platform android && expo run:android",
    "android:build": "dotenvx run -- eas build --local --platform android --profile preview",
    "android:build:prod": "dotenvx run -- eas build --local --platform android --profile prod",
    "android:build:debug": "cd android && ./gradlew assembleDebug",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "creds": "dotenvx run -- eas credentials"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@scure/bip39": "^2.0.1",
    "expo": "~54.0.30",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@types/bun": "^1.3.2",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "tsx": "^4.20.6",
    "type-fest": "^5.0.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
